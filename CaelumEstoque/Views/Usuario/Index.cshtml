@model IList<CaelumEstoque.Models.Usuario>
<h1 align="center">Lista Usuários</h1>
<table class="table" border="1">
    <thead>
        
        <tr>
            <th width="10px"><h4>Código</h4></th>
            <th><h4 class="text-center">Usuário</h4></th>
            <th colspan="3"><h4 align="center">Propriedade</h4></th>
        </th>
    </thead>
    <tbody>
        @foreach(var usuarios in Model)
        {
            <tr>
                    <td><h4 align="center">@usuarios.Id</h4></td>
                    <td><h4 class="text-center">@usuarios.Nome.ToUpper()</h4></td>
                    <!--<td width="10px"><a href=""><h4>Editar</h4></a></td>-->
                    <td width="10px"><h4>@Html.ActionLink("Editar", "Editar", new { id=@usuarios.Id})</h4></td>
                    <td width="12px"><h4>@Html.ActionLink("Visualizar", "Visualizar", new { id=@usuarios.Id})</h4></td>
                <td width="12px"><h4>@Html.ActionLink("Remover", "Remover", new { id=@usuarios.Id})</h4></td>
                    <!--<td width="12px"><a href=""><h4>Remover</h4></a></td>-->
            </tr>
        }
        
    </tbody>
</table>
<input id="cadastrar" type="button" value="Cadastrar Usuario" class="btn btn-primary" onclick="PaginaCadastro();" style="position: absolute;margin-top: -28em;margin-left: 57em;" />



<script type="text/javascript" src="~/Scripts/Jquery-1.10.2.js"></script>
<script type="text/javascript">
    function PaginaCadastro() {
        window.location.assign("/Usuario/Editar/0");
    }
</script>